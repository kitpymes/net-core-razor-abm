@page
@model App.Web.User.IndexModel
@{
    Layout = "_Layout";
    ViewData["Title"] = "Usuarios";
}

<h2>User Page</h2>

<p>Only authroized users can access this page</p>

@if (User.Identity.IsAuthenticated)
{
    <table class="table table-sm">
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
}